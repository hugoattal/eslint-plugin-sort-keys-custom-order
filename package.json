{
    "name": "eslint-plugin-sort-keys-custom-order",
    "version": "1.0.3",
    "description": "Enforce keys sorting on objects and TS types in a custom order with autofix",
    "author": {
        "name": "Hugo Attal",
        "url": "https://twitter.com/HugoAttal"
    },
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "typescript"
    ],
    "main": "./dist/index.umd.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "vite build",
        "test": "vitest",
        "dev:test": "vitest --ui"
    },
    "exports": {
        ".": {
            "require": "./dist/index.umd.js",
            "import": "./dist/index.mjs"
        },
        "./*": "./*"
    },
    "devDependencies": {
        "@types/eslint": "^8.4.10",
        "@types/estree": "^1.0.0",
        "@types/jest": "^29.2.3",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/experimental-utils": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "@vitest/ui": "^0.25.3",
        "eslint": "^8.28.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-sort-keys-custom-order": "file:..\\eslint-plugin-sort-keys-custom-order",
        "rollup-plugin-visualizer": "^5.8.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
        "vite": "^3.2.4",
        "vitest": "^0.25.3"
    },
    "peerDependencies": {
        "eslint": "^8.28.0"
    },
    "files": [
        "dist/**"
    ],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order.git"
    },
    "homepage": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order#readme",
    "url": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order",
    "bugs": {
        "url": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order/issues"
    },
    "engines": {
        "node": ">=0.10.0"
    }
}
