{
    "name": "eslint-plugin-sort-keys-custom-order",
    "version": "3.0.0",
    "description": "Enforce keys sorting on objects and TS types in a custom order with autofix",
    "type": "module",
    "author": {
        "name": "Hugo Attal",
        "url": "https://twitter.com/HugoAttal"
    },
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "typescript"
    ],
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "vite build",
        "test": "vitest",
        "dev:test": "vitest --ui"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.umd.cjs",
            "import": "./dist/index.js"
        }
    },
    "devDependencies": {
        "@types/node": "^25.2.3",
        "@typescript-eslint/rule-tester": "^8.55.0",
        "@typescript-eslint/utils": "^8.55.0",
        "@vitest/ui": "^4.0.18",
        "eslint": "10.0.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-sort-keys-custom-order": "file:",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.55.0",
        "vite": "^7.3.1",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.18"
    },
    "peerDependencies": {
        "@typescript-eslint/utils": "^8.0.0",
        "eslint": "^9.0.0"
    },
    "files": [
        "dist/**"
    ],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order.git"
    },
    "homepage": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order#readme",
    "url": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order",
    "bugs": {
        "url": "https://github.com/hugoattal/eslint-plugin-sort-keys-custom-order/issues"
    }
}
